@model Department
<div class="main-panel">
    <div class="content-wrapper">
        <h1>Department</h1>
        <table class="table table">
            <thead>
                <tr>
                    <th> Id </th>
                    <th> Title </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td> @Model.Id </td>
                    <td> @Model.Title </td>
                </tr>
            </tbody>
        </table>
        <form method="post">
            <button class="btn btn-danger" asp-action="Delete" asp-route-id="@Model.Id">Delete</button>
        </form>
    </div>
    
</div>

