@model Department
<div class="main-panel">
    <div class="content-wrapper">
        <h1>Department</h1>
        <table class="table table">
            <thead>
                <tr>
                    <th> Id </th>
                    <th> Title </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td> @Model.Id </td>
                    <td> @Model.Title </td>
                </tr>
            </tbody>
        </table>
    </div>
    <!-- content-wrapper ends -->
</div>
<!-- main-panel ends -->
