@model Doctor
<div class="main-panel">
    <div class="content-wrapper">
        <h1>Doctor</h1>
        <table class="table table">
            <thead>
                <tr>
                    <th> Id </th>
                    <th> First Name </th>
                    <th> Last Name </th>
                    <th> Department Id </th>
                    <th> Image Url </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td> @Model.Id </td>
                    <td> @Model.FirstName </td>
                    <td> @Model.LastName </td>
                    <td> @Model.Department.Title </td>
                    <td><div width="200" height="200"><img src="~/Upload/@Model.ImageUrl" /></div></td>
                </tr>
            </tbody>
        </table>
    </div>

</div>

